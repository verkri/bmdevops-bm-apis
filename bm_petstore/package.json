{
    "name": "@bankmonitor/petstore",
    "version": "1.0.6",
    "files": [
        "openapi",
        "openapi-ts",
        "*.yml"
    ],
    "scripts": {
        "gen:openapi-java": "openapi-generator-cli generate -i ./bm_pets.yml -g typescript-fetch -o ./openapi --additional-properties=supportsES6=true,withInterfaces=true",
        "gen:openapi-ts": "npx openapi-typescript ./bm_pets.yml -o ./openapi-ts/index.ts",
        "gen": "npm run gen:openapi-java && npm run gen:openapi-ts"
    },
    "devDependencies": {
        "@openapitools/openapi-generator-cli": "^2.7.0",
        "openapi-typescript": "^6.7.3"
    },
    "dependencies": {
        "openapi-fetch": "^0.8.2",
        "tsc": "^2.0.4",
        "typescript": "^5.3.3"
    }
}
